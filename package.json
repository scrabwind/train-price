{
  "name": "train-price",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun --bun nuxt build --preset bun",
    "dev": "bun --bun nuxt dev --dotenv .env.local",
    "generate": "bun --bun nuxt generate",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "bun migrate.ts",
    "studio": "drizzle-kit studio",
    "preview": "bun --bun nuxt preview",
    "postinstall": "bun --bun nuxt prepare && bun db:generate && bun db:migrate",
    "rerun": "bun local/delete.ts && bun db:generate && bun db:migrate"
  },
  "dependencies": {
    "@iconify-json/ic": "^1.1.17",
    "@nuxt/ui": "^2.14.2",
    "@playwright/browser-chromium": "^1.42.1",
    "@playwright/browser-firefox": "^1.42.1",
    "@playwright/browser-webkit": "^1.42.1",
    "db0": "^0.1.4",
    "drizzle-orm": "^0.29.5",
    "echarts": "^5.5.0",
    "nuxt": "^3.11.0",
    "pinia": "^2.1.7",
    "playwright": "^1.42.1",
    "vue": "^3.4.21",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@types/bun": "latest",
    "drizzle-kit": "^0.20.14"
  },
  "module": "index.ts",
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
